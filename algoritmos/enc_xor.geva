//algoritmo de excriptaci贸n con xor

//r0 contiene la direcci贸n de la clave
//v0 clave
ci  #3, r0
cie r0, #16, r0
cv  r0, v0

//r1 contiene la direcci贸n en al imagen original
//inicia en 0
ci  #0, r1

//r2 contiene la direcci贸n en la imagen encriptada
ci  #2, r2
cie r2, #16, r2

@repetir 8190
    //carga pixeles de la imagen original
    cv   r1, v1
    xorv v0, v1, v2
    gv   v2, r2
    si   r1, #2, r1
    si   r2, #2, r2
@finrepetir